<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Back">
        <Item>
          <IdeaActionID>Back</IdeaActionID>
          <ShortCut>'Button4 Click' or 'Ctrl+Comma'</ShortCut>
          <Description>Back</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Build">
        <Item>
          <IdeaActionID>Build</IdeaActionID>
          <ShortCut>'Ctrl+F9'</ShortCut>
          <Description>Build</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find Action...">
        <Item>
          <IdeaActionID>GotoAction</IdeaActionID>
          <ShortCut>'Ctrl+Shift+A'</ShortCut>
          <Description>Find Action...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find Usages">
        <Item>
          <IdeaActionID>FindUsages</IdeaActionID>
          <ShortCut>'Alt+F7'</ShortCut>
          <Description>Find Usages</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'Alt+1'</ShortCut>
          <Description>Project</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'Simulation.DwaThetaStarRun'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'Ctrl+F5'</ShortCut>
          <Description>Rerun 'Simulation.DwaThetaStarRun'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run">
        <Item>
          <IdeaActionID>ActivateRunToolWindow</IdeaActionID>
          <ShortCut>'Alt+4'</ShortCut>
          <Description>Run</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Simulation.DwaThetaStarRun'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Shift+F10'</ShortCut>
          <Description>Run 'Simulation.DwaThetaStarRun'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Structure">
        <Item>
          <IdeaActionID>ActivateStructureToolWindow</IdeaActionID>
          <ShortCut>'Alt+2'</ShortCut>
          <Description>Structure</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Update PlantUml Diagrams - changes only">
        <Item>
          <IdeaActionID>PlantUML.Update</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+F'</ShortCut>
          <Description>Update PlantUml Diagrams - changes only</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>